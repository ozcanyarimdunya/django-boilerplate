[report]
skip_covered = True
show_missing = True
omit =
    manage.py
    source/urls.py
    source/wsgi.py
    source/apps/*/admin.py
    source/apps/*/tests.py
    source/apps/*/management/commands/*
    source/apps/*/migrations/*
    source/apps/*/urls.py
    source/core/tests/*
    source/settings/*
